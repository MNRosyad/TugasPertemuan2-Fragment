<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Pertemuan2Fragment" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/themeTwo</item>
        <item name="colorPrimaryVariant">@color/themeThree</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondOne</item>
        <item name="colorSecondaryVariant">@color/secondTwo</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="regularButton">
        <item name="android:background">@color/themeThree</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>